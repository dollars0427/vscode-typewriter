{
  "name": "vscode-typewriter",
  "displayName": "vscode-typewriter",
  "description": "Make VS Code sound like a typewriter",
  "version": "0.0.4",
  "publisher": "jasonpoon",
  "engines": {
    "vscode": "^1.4.0"
  },
  "license": "MIT",
  "keywords": [
    "typewriter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jpoon/vscode-typewriter.git"
  },
  "homepage": "https://github.com/jpoon/vscode-typewriter",
  "bugs": {
    "url": "https://github.com/jpoon/vscode-typewriter/issues"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "keybindings": [
            { "key": "a", "command": "extension.typewriter_a", "when": "editorTextFocus" },
            { "key": "b", "command": "extension.typewriter_b", "when": "editorTextFocus" },
            { "key": "c", "command": "extension.typewriter_c", "when": "editorTextFocus" },
            { "key": "d", "command": "extension.typewriter_d", "when": "editorTextFocus" },
            { "key": "e", "command": "extension.typewriter_e", "when": "editorTextFocus" },
            { "key": "f", "command": "extension.typewriter_f", "when": "editorTextFocus" },
            { "key": "g", "command": "extension.typewriter_g", "when": "editorTextFocus" },
            { "key": "h", "command": "extension.typewriter_h", "when": "editorTextFocus" },
            { "key": "i", "command": "extension.typewriter_i", "when": "editorTextFocus" },
            { "key": "j", "command": "extension.typewriter_j", "when": "editorTextFocus" },
            { "key": "k", "command": "extension.typewriter_k", "when": "editorTextFocus" },
            { "key": "l", "command": "extension.typewriter_l", "when": "editorTextFocus" },
            { "key": "m", "command": "extension.typewriter_m", "when": "editorTextFocus" },
            { "key": "n", "command": "extension.typewriter_n", "when": "editorTextFocus" },
            { "key": "o", "command": "extension.typewriter_o", "when": "editorTextFocus" },
            { "key": "p", "command": "extension.typewriter_p", "when": "editorTextFocus" },
            { "key": "q", "command": "extension.typewriter_q", "when": "editorTextFocus" },
            { "key": "r", "command": "extension.typewriter_r", "when": "editorTextFocus" },
            { "key": "s", "command": "extension.typewriter_s", "when": "editorTextFocus" },
            { "key": "t", "command": "extension.typewriter_t", "when": "editorTextFocus" },
            { "key": "u", "command": "extension.typewriter_u", "when": "editorTextFocus" },
            { "key": "v", "command": "extension.typewriter_v", "when": "editorTextFocus" },
            { "key": "w", "command": "extension.typewriter_w", "when": "editorTextFocus" },
            { "key": "x", "command": "extension.typewriter_x", "when": "editorTextFocus" },
            { "key": "y", "command": "extension.typewriter_y", "when": "editorTextFocus" },
            { "key": "z", "command": "extension.typewriter_z", "when": "editorTextFocus" },

            { "key": "Shift+a", "command": "extension.typewriter_A", "when": "editorTextFocus" },
            { "key": "Shift+b", "command": "extension.typewriter_B", "when": "editorTextFocus" },
            { "key": "Shift+c", "command": "extension.typewriter_C", "when": "editorTextFocus" },
            { "key": "Shift+d", "command": "extension.typewriter_D", "when": "editorTextFocus" },
            { "key": "Shift+e", "command": "extension.typewriter_E", "when": "editorTextFocus" },
            { "key": "Shift+f", "command": "extension.typewriter_F", "when": "editorTextFocus" },
            { "key": "Shift+g", "command": "extension.typewriter_G", "when": "editorTextFocus" },
            { "key": "Shift+h", "command": "extension.typewriter_H", "when": "editorTextFocus" },
            { "key": "Shift+i", "command": "extension.typewriter_I", "when": "editorTextFocus" },
            { "key": "Shift+j", "command": "extension.typewriter_J", "when": "editorTextFocus" },
            { "key": "Shift+k", "command": "extension.typewriter_K", "when": "editorTextFocus" },
            { "key": "Shift+l", "command": "extension.typewriter_L", "when": "editorTextFocus" },
            { "key": "Shift+m", "command": "extension.typewriter_M", "when": "editorTextFocus" },
            { "key": "Shift+n", "command": "extension.typewriter_N", "when": "editorTextFocus" },
            { "key": "Shift+o", "command": "extension.typewriter_O", "when": "editorTextFocus" },
            { "key": "Shift+p", "command": "extension.typewriter_P", "when": "editorTextFocus" },
            { "key": "Shift+q", "command": "extension.typewriter_Q", "when": "editorTextFocus" },
            { "key": "Shift+r", "command": "extension.typewriter_R", "when": "editorTextFocus" },
            { "key": "Shift+s", "command": "extension.typewriter_S", "when": "editorTextFocus" },
            { "key": "Shift+t", "command": "extension.typewriter_T", "when": "editorTextFocus" },
            { "key": "Shift+u", "command": "extension.typewriter_U", "when": "editorTextFocus" },
            { "key": "Shift+v", "command": "extension.typewriter_V", "when": "editorTextFocus" },
            { "key": "Shift+w", "command": "extension.typewriter_W", "when": "editorTextFocus" },
            { "key": "Shift+x", "command": "extension.typewriter_X", "when": "editorTextFocus" },
            { "key": "Shift+y", "command": "extension.typewriter_Y", "when": "editorTextFocus" },
            { "key": "Shift+z", "command": "extension.typewriter_Z", "when": "editorTextFocus" }
    ]},
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^1.8.5",
    "vscode": "^0.11.0"
  },
  "dependencies": {
    "play-sound": "0.0.9"
  }
}
